    "css-loader": "^2.1.0",
    "graphql": "^14.1.1",
    "i18next": "^15.0.4",
    "lodash": "^4.17.11",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-i18next": "^10.2.0",
